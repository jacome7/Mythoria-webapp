{
  "name": "mythoria-webapp",
  "version": "0.6",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "cross-env NODE_OPTIONS=--disable-warning=DEP0040 next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.cjs,.mjs,.ts,.tsx --max-warnings=0",
    "lint:fix": "eslint . --ext .js,.cjs,.mjs,.ts,.tsx --fix",
    "typecheck": "rimraf .next/types && tsc --noEmit",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "format_fix": "npm run format:fix",
    "postinstall": "patch-package",
    "logs": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/logs.ps1",
    "i18n:prune:pt-PT:common": "tsx scripts/prune-unused-keys.ts pt-PT common.json",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx -r dotenv/config ./src/db/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:reset": "tsx -r dotenv/config ./src/db/reset.ts",
    "db:setup": "npm run db:reset && npm run db:push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx -r dotenv/config ./src/db/seed.ts",
    "deploy:production": "gcloud beta builds submit --config cloudbuild.yaml",
    "deploy": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/deploy.ps1",
    "deploy:fast": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/deploy.ps1 -Fast",
    "docs:serve": "npx serve docs/",
    "i18n:parity": "tsx scripts/check-locale-parity.ts",
    "i18n:keys": "tsx scripts/generate-translation-keys.ts",
    "test": "cross-env NODE_OPTIONS=--no-deprecation jest",
    "test:e2e": "playwright test",
    "test:e2e:anon": "cross-env SKIP_AUTH_SETUP=1 playwright test tests/playwright/missing-translations_anonymous.spec.ts --workers=1",
    "check:env": "tsx ./scripts/check-env-parity.ts"
  },
  "sideEffects": false,
  "dependencies": {
    "@clerk/backend": "^2.29.5",
    "@clerk/localizations": "^3.35.2",
    "@clerk/nextjs": "^6.36.10",
    "@ducanh2912/next-pwa": "^10.2.9",
    "@google-cloud/pubsub": "^5.2.2",
    "@google-cloud/storage": "^7.18.0",
    "@lexical/history": "^0.39.0",
    "@lexical/html": "^0.39.0",
    "@lexical/react": "^0.39.0",
    "@lexical/rich-text": "^0.39.0",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@revolut/checkout": "^1.1.24",
    "@tailwindcss/typography": "^0.5.19",
    "daisyui": "^5.5.14",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "image-size": "^2.0.2",
    "lexical": "^0.39.0",
    "libphonenumber-js": "^1.12.35",
    "mermaid": "^10.9.5",
    "next": "^16.1.5",
    "next-intl": "^4.7.0",
    "next-mdx-remote": "^5.0.0",
    "pg": "^8.17.2",
    "postgres": "^3.4.8",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-easy-crop": "^5.5.6",
    "react-icons": "^5.5.0",
    "react-type-animation": "^3.2.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "svix": "^1.84.1",
    "tsx": "^4.21.0",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@lingual/i18n-check": "^0.8.19",
    "@playwright/test": "^1.58.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/image-size": "^0.8.0",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/pg": "^8.16.0",
    "@types/react-dom": "^19.2.3",
    "baseline-browser-mapping": "^2.9.18",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "^16.1.5",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "js-yaml": "^4.1.1",
    "knip": "^5.82.1",
    "patch-package": "^8.0.1",
    "prettier": "^3.8.1",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "overrides": {
    "rimraf": "^6.0.1",
    "@jridgewell/sourcemap-codec": "^1.5.4",
    "glob": "^11.0.2",
    "esbuild": "^0.25.9"
  }
}
