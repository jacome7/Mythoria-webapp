{
  "name": "mythoria-webapp",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "node scripts/verify-translations.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx -r dotenv/config ./src/db/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:reset": "tsx -r dotenv/config ./src/db/reset.ts",
    "db:setup": "npm run db:reset && npm run db:push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx -r dotenv/config ./src/db/seed.ts",
    "db:seed:pricing": "tsx -r dotenv/config ./scripts/seed-pricing.ts",
    "test:print-providers": "node scripts/test-print-providers.js",
    "deploy:staging": "gcloud builds submit --config cloudbuild-staging.yaml",
    "deploy:production": "gcloud builds submit --config cloudbuild.yaml",
    "deploy:win": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1",
    "docs:serve": "npx serve docs/"
  },
  "sideEffects": false,
  "dependencies": {
    "@clerk/localizations": "^3.18.0",
    "@clerk/nextjs": "^6.24.0",
    "@floating-ui/react": "^0.27.13",
    "@google-cloud/pubsub": "^5.1.0",
    "@google-cloud/storage": "^7.16.0",
    "@google-cloud/vertexai": "^1.10.0",
    "@google/genai": "^1.9.0",
    "@revolut/checkout": "^1.1.21",
    "@tiptap/extension-color": "^2.25.0",
    "@tiptap/extension-image": "^2.25.0",
    "@tiptap/extension-link": "^2.25.0",
    "@tiptap/extension-text-align": "^2.25.0",
    "@tiptap/extension-text-style": "^2.25.0",
    "@tiptap/pm": "^2.25.0",
    "@tiptap/react": "^2.25.0",
    "@tiptap/starter-kit": "^2.25.0",
    "@types/jsdom": "^21.1.7",
    "@types/pg": "^8.15.4",
    "daisyui": "^5.0.46",
    "dotenv": "^17.2.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.2",
    "jsdom": "^26.1.0",
    "next": "^15.3.5",
    "next-intl": "^4.3.4",
    "next-pwa": "^5.6.0",
    "pg": "^8.16.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-type-animation": "^3.2.0",
    "svix": "^1.69.0",
    "tsx": "^4.20.3",
    "uuid": "^11.1.0",
    "zod": "^4.0.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/next-pwa": "^5.6.9",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "^15.3.5",
    "playwright": "^1.53.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
