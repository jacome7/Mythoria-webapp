{
  "name": "mythoria-webapp",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "rimraf .next/types && tsc --noEmit",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "logs": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/get-cloud-run-logs.ps1",
    "i18n:prune:pt-PT:common": "tsx scripts/prune-unused-keys.ts pt-PT common.json",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx -r dotenv/config ./src/db/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:reset": "tsx -r dotenv/config ./src/db/reset.ts",
    "db:setup": "npm run db:reset && npm run db:push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx -r dotenv/config ./src/db/seed.ts",
    "deploy:production": "gcloud beta builds submit --config cloudbuild.yaml",
    "deploy": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/deploy.ps1",
    "deploy:fast": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/deploy.ps1 -Fast",
    "docs:serve": "npx serve docs/",
    "i18n:parity": "tsx scripts/check-locale-parity.ts",
    "i18n:keys": "tsx scripts/generate-translation-keys.ts",
    "test": "cross-env NODE_OPTIONS=--no-deprecation jest",
    "check:env": "node -r ts-node/register ./scripts/check-env-parity.ts"
  },
  "sideEffects": false,
  "dependencies": {
    "@clerk/localizations": "^3.20.1",
    "@clerk/nextjs": "^6.25.4",
    "@ducanh2912/next-pwa": "^10.2.9",
    "@google-cloud/pubsub": "^5.1.0",
    "@google-cloud/storage": "^7.16.0",
    "@lexical/history": "^0.33.1",
    "@lexical/html": "^0.33.1",
    "@lexical/react": "^0.33.1",
    "@lexical/rich-text": "^0.33.1",
    "@revolut/checkout": "^1.1.21",
    "@tailwindcss/typography": "^0.5.16",
    "daisyui": "^5.0.46",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.3",
    "lexical": "^0.33.1",
    "mermaid": "^11.9.0",
    "next": "^15.4.2",
    "next-intl": "^4.3.4",
    "next-mdx-remote": "^5.0.0",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-easy-crop": "^5.0.7",
    "react-type-animation": "^3.2.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "svix": "^1.69.0",
    "tsx": "^4.20.3",
    "uuid": "^11.1.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@lingual/i18n-check": "^0.8.4",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "cross-env": "^10.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^9",
    "eslint-config-next": "^15.4.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.3.3",
    "tailwindcss": "^4",
    "ts-jest": "^29.2.5",
    "typescript": "^5",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "overrides": {
    "@types/long": false,
    "node-domexception": false,
    "rimraf": "^6.0.1",
    "@jridgewell/sourcemap-codec": "^1.5.4",
    "inflight": false,
    "@esbuild-kit/esm-loader": false,
    "@esbuild-kit/core-utils": false,
    "esbuild": "^0.25.9",
    "tsx": "^4.20.3"
  }
}
